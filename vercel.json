{
  "installCommand": "npm install && npm --prefix fletes-driver-pwa install",
  "buildCommand": "npm --prefix fletes-driver-pwa run build",
  "outputDirectory": "fletes-driver-pwa/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/driver",
      "destination": "/index.html"
    },
    {
      "source": "/driver/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/job/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/index.html"
    }
  ]
}
